pipeline{
    agent any
    stages{
        stage("compile"){
            steps{
                script{
                    echo "compiling the code"
                }
            }
        }
        stage("test"){
            steps{
                script{
                    echo "test the code"
                }
            }
        }
        stage("package"){
            steps{
                script{
                    echo"package the code"
                }
            }
        }
    }    
}    